{
  "name": "@use-gpu/app",
  "version": "0.11.3",
  "main": "src/index.ts",
  "author": "Steven Wittens <steven@acko.net>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@use-gpu/core": "*",
    "@use-gpu/layout": "*",
    "@use-gpu/live": "*",
    "@use-gpu/map": "*",
    "@use-gpu/inspect": "*",
    "@use-gpu/present": "*",
    "@use-gpu/plot": "*",
    "@use-gpu/scene": "*",
    "@use-gpu/shader": "*",
    "@use-gpu/state": "*",
    "@use-gpu/webgpu": "*",
    "@use-gpu/workbench": "*",
    "@use-gpu/wgsl": "*"
  },
  "scripts": {
    "build": "yarn run build:wgsl; yarn run build:pkg",
    "build:pkg": "NPM_PACKAGE=app ../../scripts/build-mjs-cjs.sh",
    "build:wgsl": "../shader/bin/shader2ts.js --noEmit --typeDef src",
    "test": "MODULE_ENV=cjs jest --watch",
    "lint": "eslint src --ext ts",
    "coverage": "echo 'jest --collect-coverage' disabled",
    "types": "tsc --noEmit",
    "types:watch": "yarn run types -- --watch"
  },
  "keywords": [
    "use-gpu",
    "use-gpu-dev"
  ]
}