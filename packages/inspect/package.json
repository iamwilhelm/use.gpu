{
  "name": "@use-gpu/inspect",
  "version": "0.0.1",
  "main": "src/index.ts",
  "author": "Steven Wittens <steven@acko.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --watch",
    "lint": "eslint src --ext ts",
    "coverage": "echo 'jest --collect-coverage' disabled",
    "types": "tsc --noEmit",
    "types:watch": "yarn run types -- --watch",
    "build": "yarn run build:js && yarn run build:types && yarn run build:pkg",
    "build:types": "rm tsconfig.tsbuildinfo; tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir ../../build/packages/inspect/src --extensions \".ts\" --ignore \"src/**/__mocks__/**/*.js\" --ignore \"src/**/*.test.ts\"",
    "build:pkg": "cp package.json ../../build/packages/inspect"
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "styled-components": "^5.3.3"
  }
}
