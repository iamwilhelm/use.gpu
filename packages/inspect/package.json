{
  "name": "@use-gpu/inspect",
  "version": "0.0.1",
  "main": "src/index.ts",
  "author": "Steven Wittens <steven@acko.net>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --watch",
    "lint": "eslint src --ext ts",
    "coverage": "echo 'jest --collect-coverage' disabled",
    "types": "tsc --noEmit",
    "types:watch": "yarn run types -- --watch",
    "build": "yarn run build:js && yarn run build:types && yarn run build:css && yarn run build:pkg",
    "build:types": "rm tsconfig.tsbuildinfo; tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir ../../build/packages/inspect/src --extensions \".ts,.tsx\" --ignore \"src/**/__mocks__/**/*.js\" --ignore \"src/**/*.test.ts\"",
    "build:pkg": "cp package.json ../../build/packages/inspect",
    "build:css": "cp theme.css ../../build/packages/inspect"
  },
  "dependencies": {
    "@radix-ui/react-slider": "^0.1.4",
    "@radix-ui/react-tabs": "^0.1.5",
    "@stitches/react": "^1.2.7"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "typedocOptions": {
    "name": "inspect",
    "readme": "./README.md"
  }
}
